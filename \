import React, {useEffect} from 'react';

const API = 'https://api.github.com/search/users';

export const Home = () => {

    const getUsers = async () => {
        const res = await fetch(`${API}/guillermoferraz`);
        const data = await res.json();
        console.log(data)
    }
    useEffect(() =>{
        getUsers();
    })

    return(
        <div>
            Home
        </div>
    )
}
